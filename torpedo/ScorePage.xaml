<Page x:Class="NationalInstruments.ScorePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NationalInstruments"
      mc:Ignorable="d" 
      d:DesignHeight="420" d:DesignWidth="800"
      Title="ScorePage">

    <Grid>
        <DataGrid x:Name="Datagrid" HorizontalAlignment="Left" Height="420" VerticalAlignment="Top" Width="800" d:ItemsSource="{d:SampleData ItemCount=5}"/>

    </Grid>
</Page>
